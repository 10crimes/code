TODIR="$JPATH/images/pp/$1-$2"
mkdir -p "$TODIR"
convert origover.bmp -geometry 100 "$TODIR/origover.jpg"
cp gplfit.ps "$TODIR"
convert recover.bmp -geometry 100 "$TODIR/recover.jpg"
convert ppmap.bmp -geometry 80 "$TODIR/ppmap.jpg"
# convert bestppgaps.bmp -geometry 200 "$TODIR/bestppgaps.jpg"
ln -s "$JPATH/images/$1$2.bmp" "$TODIR"
