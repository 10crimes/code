TODIR="$JPATH/images/pp/$1-$2"
mkdir -p "$TODIR"
convert origover.bmp -geom 100 "$TODIR/origover.jpg"
cp gplfit.ps "$TODIR"
convert recover.bmp -geom 100 "$TODIR/recover.jpg"
convert ppmap.bmp -geom 80 "$TODIR/ppmap.jpg"
# convert bestppgaps.bmp -geom 200 "$TODIR/bestppgaps.jpg"
ln -s "$JPATH/images/$1$2.bmp" "$TODIR"
