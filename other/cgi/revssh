#!/bin/sh

echo "Content-type: text/html"
echo

. cgilib
export JPATH=/home/joey/j
. /home/joey/j/startj-simple

## You don't need the three lines above,
## if you can get a good alternative for this line:
OUTPUT=`getcgi "OUTPUT"`
## (which doesn't convert from cgi properly anyway!)

if test "$OUTPUT"; then
	echo "$OUTPUT" >> /tmp/revssh-client-output.txt
fi

# cat /tmp/revssh-client-input.txt && rm -f /tmp/revssh-client-input.txt
cat /tmp/revssh-client-input.txt && printf "" > /tmp/revssh-client-input.txt
