AUTH="$1"
if [ "$AUTH" = "DUNNO_AUTH" ]
then echo "?.?"
else
	COUNT=`cat ~/.scanpug/*.picks | grep " $AUTH " | wc -l`
	if [ "$COUNT" -gt 0 ]
	then
		SUM=`cat ~/.scanpug/*.picks | grep " $AUTH " | takecols 1 | awksum`
		STRENGTH=$((10*SUM/COUNT))
		STRENGTH=`echo "$STRENGTH" | sed 's+^.+\0.+'`
		echo "$STRENGTH"
	else
		echo "?.?"
	fi
fi
