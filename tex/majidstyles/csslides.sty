\def\@setpage#1#2{
  \@swidth            #1\relax      \@sheight              #2\relax
  \textheight         \@sheight     \advance\textheight    -40pt
  \headsep           -\@sheight     \advance\headsep       -13pt
  \headheight         \@sheight     \advance\headheight     43pt
  \textwidth          \@swidth      \advance\textwidth     -40pt
  \oddsidemargin     -\textwidth    \advance\oddsidemargin \@pwidth
  \divide\oddsidemargin\tw@         \evensidemargin        \oddsidemargin
  \topmargin          \@pheight     \advance\topmargin -20pt
  \advance\topmargin -\@sheight     \divide\topmargin\tw@
}
